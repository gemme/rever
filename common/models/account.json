{
  "name": "Account",
  "plural": "Accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": true,
    "ResetPassword": true
  },
  "properties": {
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "string"
    },
    "photo": {
      "type": "object"
    },
    "demographic": {
      "type": "object"
    },
    "confirmed": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "organization": {
      "type": "belongsTo",
      "model": "Organization",
      "foreignKey": ""
    },
    "revs": {
      "type": "hasMany",
      "model": "Rev",
      "foreignKey": "",
      "through": "AccountRev"
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": ""
    },
    "challenges": {
      "type": "hasMany",
      "model": "Challenge",
      "foreignKey": ""
    },
    "invitations": {
      "type": "hasMany",
      "model": "Invitation",
      "foreignKey": "senderId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "welcome"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "changePassword"
    }
  ],
  "methods": {}
}
